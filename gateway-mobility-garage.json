{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Mobility Garage Gateway",
    "description" : "This API is used to integrate garage software provider with JAROWA ENAB.LE platform via a gateway.",
    "version" : "1.0"
  },
  "paths" : {
    "/api/v1/CaseData" : {
      "post" : {
        "tags" : [ "CaseDataStore" ],
        "summary" : "Stores all information into a local memory storage by the key CaseData.Id.",
        "requestBody" : {
          "description" : "prefilling data passed from garage software provider.",
          "content" : {
            "application/json-patch+json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Enable.Plugin.Mobility.Garage.Models.Case"
              }
            },
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Enable.Plugin.Mobility.Garage.Models.Case"
              }
            },
            "text/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Enable.Plugin.Mobility.Garage.Models.Case"
              }
            },
            "application/*+json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Enable.Plugin.Mobility.Garage.Models.Case"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "if the data is successfully stored."
          },
          "500" : {
            "description" : "if any other error occures."
          }
        }
      }
    }
  },
